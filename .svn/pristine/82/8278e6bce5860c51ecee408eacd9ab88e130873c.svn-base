page-studio-identity {
    img{
        object-fit: cover;
    }
    .item-inner{
        border-bottom:0 !important;
    }
    .list-ios{
        margin:0 !important;
    }
    .item-ios{
        padding:0 !important;
        background: transparent !important;
    }
    .studio_box{
        width:100%;
        height:auto;
        overflow: hidden;
        .signUp_title{
            h1{
                font-size: 1.7rem;
                line-height: 2rem;
                color:#333;
            }
            input{
                line-height: 3rem;
                height:3rem;
                background: #fff;
                border-radius:4px;
                padding:0 1rem;
                width:100%;
                border:1px solid #d9d9d9;
                margin-bottom: 1rem;
            }
            textarea{
                border:1px solid #d9d9d9;
                background:#fff;
                height:10rem;
                width:100%;
                padding:1rem;
                line-height: 2rem;
                border-radius:4px;
            }
        }
    }
    

    // 上传海报
    ion-app [padding] + [padding] {
        padding-top: 0;
    }
    
    [no-padding] {
        padding: 0;
    }
    
    [padding-left] {
       padding-left: 10px;
    }
    
    [padding-right] {
        padding-right: 10px;
    }
    
    [padding-top] {
        padding-top: 0.2rem;
    }
    
    [paddding-bottom] {
        padding-bottom: 0.2rem;
    }
    
    [no-margin] {
        margin: 0;
    }
    
    // [bold] {
    //     font-weight: bold;
    // }
    
    [absolute] {
        position: absolute;
    }
    
    [relative] {
        position: relative;
    }
    
    [top] {
        top: 0;
    }
    
    [bottom] {
        bottom: 0;
    }
    
    [left] {
        left: 0;
    }
    
    [right] {
        right: 0;
    }
    
    [center] {
        top: 50%;
        left: 50%;
        transform: translate3d(-50%, -50%, 0);
    }
    
    [nowrap] {
        max-width: 100%;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }
    
    [flex] {
        display: flex;
        justify-content: center;
        align-items: center;
        align-content: center;
    }
    [flex-column] {
        flex-direction: column
    }
    
    [flex-start] {
        justify-content: flex-start
    }
    
    [space-between] {
        justify-content: space-between
    }
    
    [flex-end] {
        justify-content: flex-end
    }
    
    [align-start] {
        align-items: flex-start;
        align-content: flex-start
    }
    
    [flex-wrap]{
        flex-wrap:wrap;
    }
    
    [font-size-12] {
        font-size: 1.2rem;
    }
    
    .item.clear,.item[clear] {
        border: none !important;
        .item-inner {
            padding-right: 0;
            border: none;
            .label {
                margin: 0;
            }
        }
    }
    
    .list {
        &[long-lines] {
            .item {
                &:not(:last-child):after {
                    content: '';
                    display: block;
                    position: absolute;
                    top: 100%;
                    width: 100%;
                    border-bottom: 1px solid #c8c7cc;
                    transform: translateY(-0.55px);
                }
                .item-inner {
                    border-bottom: 0;
                }
            }
        }
    }
    
    /*css reset*/
    .button-ios:not([round]) {
        border-radius: 8px;
    }
    
    p {
        margin: 0;
    }
    
    ul {
        padding: 0;
        margin: 0;
        list-style: none;
    }
    
    button, .button {
        line-height: 2;
    }
    .evaluateBox{
        margin-bottom:1.8rem;
        font-size:1.5rem;
        // padding:0 1.5rem;
        label{
            float: left;
            width:20%;
        }
        textarea{
            width:80%;
            border:1px solid #ccc;
            background:#f8f8f8;
            padding:0.5rem;
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }
        .scoreCell{
            margin-bottom:1rem;
            .score{
                float: left;
                span{
                    float: left;
                    margin-right:10px;
                    color:#15b72e;
                    font-size:1.6rem;
                    img{
                        width:26px;
                        float:left;
                        margin-top:-4px;
                    }
                }
            }
        }
        .imgBox{
            width:100%;
            border:1px solid #ccc;
            background:#fff;
            padding:0.5rem;
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
            float: left;
            ul{
                float: left;
                li{
                    float: left;
                    margin:0 1rem 1rem 0;
                    width:6rem;
                    height:6rem;
                    img{
                    width:100%;
                    }
                }
            }
            .add{
                float: left;
                margin-right:1rem;
                width:5rem;
                height:5rem;
                background:#9a9a9a;
                img{
                    float: left;
                    width:3rem;
                    height:3rem;
                    margin:1rem 0 0 1rem;
                }
            }
        }
    }
    
    .photo_container {
        display: flex;
        justify-content: flex-start;
    }
    .stylephotos {
        .label{
            overflow: visible;
        }
        display: flex;
        flex-wrap: wrap;
        margin-top: 5px;
        .photo {
            width: 100%;
            height: 8rem;
            position: relative;
            border-radius: 0.5rem;
            background-color: #fff;
            img {
                width: 100%;
                height: 8rem;
            }
            .close {
                position: absolute;
                text-align: center;
                display: block;
                //line-height: $closeWidth;
                font-size: 3rem*0.9;
                top: 1rem;
                right: 1rem;
                width: 1rem;
                height: 1rem;
                border-radius: 50%;
                border: 1px solid gray;
                //background-color:#F0EFF5;
                background-color: #FFF;
                color: gray;
            }
            .close:before {
                content: '';
                display: block;
                position: absolute;
                height: 0.2rem;
                width: 1rem*0.64;
                margin-left: -1rem*0.32;
                margin-top: -0.1rem;
                top: 50%;
                left: 50%;
                background: gray;
                transform: rotate(45deg);
            }
            .close:after {
                content: '';
                display: block;
                position: absolute;
                height: 0.2rem;
                width: 1rem*0.64;
                margin-left: -1rem*0.32;
                margin-top: -0.1rem;
                top: 50%;
                left: 50%;
                background: gray;
                transform: rotate(-45deg);
            }
        }
        .upload {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            font-size: 3rem;
            color: #AEAEAE;
            background:#EDEDED;
            span {
                font-size: 1.2rem;
                color: #666;
                white-space: nowrap;
            }
            form {
                display: none;
            }
        }
    }
}
