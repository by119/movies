page-setting {
    .setting_box{
        width:100%;
        .setting_mid{
            width:100%;
            height:auto;
            line-height: 4rem;
            padding:0 1rem;
            border-bottom:1px solid #d9d9d9;
            background:#fff;
            height:4rem;
            span{
                float:left;
                display: block;
            }
            ion-icon{
                float:right;
                line-height: 4rem;
            }
        }
        .loginout{
            width:100%;
            height:4rem;
            button{
                display: block;
                background:linear-gradient(to right, #49afa4, #14666e);
                width:94%;
                margin:10rem 3% 0;
                height:4rem;
                color:#fff;
                font-size: 1.7rem;
                border-radius:4px;
            }
        }
    }
}
