page-demand-upload {
    img{
        object-fit: cover;
    }
    .addrole_sex{
        .item-ios{
            padding: 0 !important;
        }
    }
    .addrole_name{
        h3{
            font-size: 1.6rem;
            color:#333;
            // line-height: 2.5rem;
        }
        input{
            line-height: 3rem;
            width:100%;
            border-radius:4px;
            border:1px solid #d9d9d9;
            background:#fff;
            padding:0 1rem;
        }
    }
    .addrole_sex{
        h3{
            font-size: 1.6rem;
            color:#333;
            line-height: 2.5rem;
            display: block;
            float:left;
            margin-right: 1rem;
        }
    }
    // 添加角色弹窗
    .addRole{
        width:100%;
        height:100%;
        overflow: hidden;
        background:rgba(0,0,0,.7);
        position: fixed;
        z-index:9999;
        .addRoleBox{
            width:80%;
            height:auto;
            overflow: hidden;
            background: #fff;
            border-radius:4px;
            position: absolute;
            top: 50%;
            left:50%;
            transform: translate(-50%,-50%);
            .addRole_title{
                width:100%;
                height:3rem;
                line-height: 3rem;
                text-align: center;
                color:#fff;
                background: linear-gradient(to right, #49afa4, #14666e);
            }
            .addRole_close{
                position: absolute;
                top:0.6rem;
                color:#fff;
                font-size: 1.8rem;
                right:1rem;
            }
            .addRole_content{
                margin:5%;
                textarea{
                    width:100%;
                    border-radius: 4px;
                    border:1px solid #d9d9d9;
                    padding:3%;
                    height:8rem;
                    line-height: 2rem;
                    color:#333;
                }
            }
            .addRole_Button{
                background: linear-gradient(to right, #49afa4, #14666e);
                width:100%;
                border-radius:4px;
                line-height: 3rem;
                color:#fff;
            }
        }
    }
    .roleIntro{
        margin-top: 1rem;
        .roles_text{
            padding: 0.5rem 0;
            border-bottom: 1px solid #fafafa;
            h5{
                font-size: 1.6rem;
                color:#111;
                span{
                    margin-left: 2rem;
                }
            }
        }
        p{
            color:#333;
            line-height: 2rem;
            white-space: normal;
        }
    }

    .scroll-content {
        background-color: #fafafa;
    }

    .item-ios.item-block .item-inner {
        border: unset;
    }
    .list-ios {
        margin: 0;
    }
    .logistics{
        font-size: 1.5rem;
        border-bottom:2px solid #fafafa;
    }
    .label-ios{
        // font-weight: 600;
        font-size: 1.5rem;
    }
    .select-text{
        font-size: 1.5rem;
        color:#666;
    }
    .datetime-text{
        color:#666;
        font-size: 1.5rem;
    }
    .con-all{
        width: 100%;
        overflow: hidden;
        margin-bottom: 0.2rem;
        .con-all-content{
            width: 100%;
            background: #fff;
            overflow: hidden;
            .con-all-title{
                float: left;
                font-size: 1.7rem;
                color:#333;
                // font-weight: 600;
            }
            .con-all-input{
                width: 65%;
                float: right;
                text-align: right;
                border: none;
                font-size: 1.5rem;
                color:#666;
            }
            .con-all-title1{
                width: 100%;
                font-size: 1.7rem;
                color:#333;
            }
            .upload{
                overflow: hidden;
                width: 100%;
                .icon-upload-s{
                    font-size: 8rem;
                    margin: 2rem;
                    color: #EAEAEA;
                    // font-weight: 300
                }
                .upload-ti{
                    float: right;
                    font-size: 1rem;
                    color: #4BAE9C;
                    line-height: 10rem;
                }
                .tian{
                    font-size: 2.5rem;
                }
                .upload-title{
                    color: #4BAE9C;
                    font-size: 1.5rem;
                }
            
            }
            .upload1{
                overflow: hidden;
                width: 100%;
               
                margin-top: 1rem;
                display: flex;
                justify-content:center;
                align-items:center;
                .tian{
                    font-size: 3rem;
                    color: #EAEAEA;
                    margin-top: 1rem;
                }
                .upload-title{
                    color: #4BAE9C;
                    font-size: 1.5rem;
                    margin-left: 1rem;
                    margin-top: 1rem                }
            
            }
            .con-text{
                margin-top: 1rem;
                width: 100%;
                height: 10rem;
                background: #F9F9F9;
                border: none;
                padding: 1rem;
                color:#666;
                border-radius:4px;
            }
            
        }

        // 上传海报
        ion-app [padding] + [padding] {
            padding-top: 0;
        }
        
        [no-padding] {
            padding: 0;
        }
        
        [padding-left] {
           padding-left: 10px;
        }
        
        [padding-right] {
            padding-right: 10px;
        }
        
        [padding-top] {
            padding-top: 0.2rem;
        }
        
        [paddding-bottom] {
            padding-bottom: 0.2rem;
        }
        
        [no-margin] {
            margin: 0;
        }
        
        // [bold] {
        //     font-weight: bold;
        // }
        
        [absolute] {
            position: absolute;
        }
        
        [relative] {
            position: relative;
        }
        
        [top] {
            top: 0;
        }
        
        [bottom] {
            bottom: 0;
        }
        
        [left] {
            left: 0;
        }
        
        [right] {
            right: 0;
        }
        
        [center] {
            top: 50%;
            left: 50%;
            transform: translate3d(-50%, -50%, 0);
        }
        
        [nowrap] {
            max-width: 100%;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }
        
        [flex] {
            display: flex;
            justify-content: center;
            align-items: center;
            align-content: center;
        }
        [flex-column] {
            flex-direction: column
        }
        
        [flex-start] {
            justify-content: flex-start
        }
        
        [space-between] {
            justify-content: space-between
        }
        
        [flex-end] {
            justify-content: flex-end
        }
        
        [align-start] {
            align-items: flex-start;
            align-content: flex-start
        }
        
        [flex-wrap]{
            flex-wrap:wrap;
        }
        
        [font-size-12] {
            font-size: 1.2rem;
        }
        
        .item.clear,.item[clear] {
            border: none !important;
            .item-inner {
                padding-right: 0;
                border: none;
                .label {
                    margin: 0;
                }
            }
        }
        
        .list {
            &[long-lines] {
                .item {
                    &:not(:last-child):after {
                        content: '';
                        display: block;
                        position: absolute;
                        top: 100%;
                        width: 100%;
                        border-bottom: 1px solid #c8c7cc;
                        transform: translateY(-0.55px);
                    }
                    .item-inner {
                        border-bottom: 0;
                    }
                }
            }
        }
        
        /*css reset*/
        .button-ios:not([round]) {
            border-radius: 8px;
        }
        
        p {
            margin: 0;
        }
        
        ul {
            padding: 0;
            margin: 0;
            list-style: none;
        }
        
        button, .button {
            line-height: 1;
        }
        .evaluateBox{
            margin-bottom:1.8rem;
            font-size:1.5rem;
            // padding:0 1.5rem;
            label{
                float: left;
                width:20%;
            }
            textarea{
                width:80%;
                border:1px solid #ccc;
                background:#f8f8f8;
                padding:0.5rem;
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                box-sizing: border-box;
            }
            .scoreCell{
                margin-bottom:1rem;
                .score{
                    float: left;
                    span{
                        float: left;
                        margin-right:10px;
                        color:#15b72e;
                        font-size:1.6rem;
                        img{
                            width:26px;
                            float:left;
                            margin-top:-4px;
                        }
                    }
                }
            }
            .imgBox{
                width:100%;
                border:1px solid #ccc;
                background:#fff;
                padding:0.5rem;
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                box-sizing: border-box;
                float: left;
                ul{
                    float: left;
                    li{
                        float: left;
                        margin:0 1rem 1rem 0;
                        width:6rem;
                        height:6rem;
                        img{
                        width:100%;
                        }
                    }
                }
                .add{
                    float: left;
                    margin-right:1rem;
                    width:5rem;
                    height:5rem;
                    background:#9a9a9a;
                    img{
                        float: left;
                        width:3rem;
                        height:3rem;
                        margin:1rem 0 0 1rem;
                    }
                }
            }
        }
        
        .photo_container {
            display: flex;
            justify-content: flex-start;
        }
        .stylephotos {
            .label{
                overflow: visible;
            }
            display: flex;
            flex-wrap: wrap;
            margin-top: 5px;
            .photo {
                width: 100%;
                height: 8rem;
                position: relative;
                border-radius: 0.5rem;
                background-color: #fff;
                img {
                    width: 100%;
                    height: 8rem;
                }
                .close {
                    position: absolute;
                    text-align: center;
                    display: block;
                    //line-height: $closeWidth;
                    font-size: 3rem*0.9;
                    top: 1rem;
                    right: 1rem;
                    width: 1rem;
                    height: 1rem;
                    border-radius: 50%;
                    border: 1px solid gray;
                    //background-color:#F0EFF5;
                    background-color: #FFF;
                    color: gray;
                }
                .close:before {
                    content: '';
                    display: block;
                    position: absolute;
                    height: 0.2rem;
                    width: 1rem*0.64;
                    margin-left: -1rem*0.32;
                    margin-top: -0.1rem;
                    top: 50%;
                    left: 50%;
                    background: gray;
                    transform: rotate(45deg);
                }
                .close:after {
                    content: '';
                    display: block;
                    position: absolute;
                    height: 0.2rem;
                    width: 1rem*0.64;
                    margin-left: -1rem*0.32;
                    margin-top: -0.1rem;
                    top: 50%;
                    left: 50%;
                    background: gray;
                    transform: rotate(-45deg);
                }
            }
            .upload {
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
                font-size: 3rem;
                color: #AEAEAE;
                background:#EDEDED;
                span {
                    font-size: 1.2rem;
                    color: #666;
                    white-space: nowrap;
                }
                form {
                    display: none;
                }
            }
        }
    }






    
}
