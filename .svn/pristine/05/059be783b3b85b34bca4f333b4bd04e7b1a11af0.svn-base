page-search {
    .list-ios{
        margin:0 !important;
    }
    .search_box{
        width:100%;
        height:auto;
        overflow: hidden;
        .serch_mids{
            width:100%;
            height:auto;
            overflow: hidden;
            border:1px solid #d9d9d9;
            border-radius:4rem;
            background: #fff;
            input{
                width:90%;
                height:3rem;
                line-height: 3rem;
                padding:0 1rem;
                border:none;
            }
            .ion-icon{
                width:10%;
                font-size: 1.8em !important;
                color:#d9d9d9 !important;
            }
        }
        .serch_history{
            width:100%;
            height:auto;
            overflow: hidden;
            p{
                line-height: 5rem;
                color:#555;
            }
            .serch_historyList{
                width:100%;
                height:4rem;
                line-height: 4rem;
                color:#333;
                background:#fff;
                border-bottom:1px solid #fafafa;
                padding:0 1rem;
                span{
                    float:left;
                }
                .icon_history{
                    float:right;
                    line-height: 4rem;
                }
            }
        }
    }
}
